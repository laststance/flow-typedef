{
  "parser": "babel-eslint",
  "env": {
    "es6": true
  },
  "plugins": [
    "prettier"
  ],
  "rules": {
    "prettier/prettier": [
      "error",
      {
        "singleQuote": true,
        "semi": false,
        "parser": "flow"
      }
    ],
    "jsx-a11y/href-no-hash": "off",
    "no-unused-vars": [
      "error"
    ]
  }
}
